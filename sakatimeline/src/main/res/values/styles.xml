<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeLineView">
        <attr name="timePadding" format="dimension" />
        <attr name="timeDrawable" format="color|reference" />
        <attr name="timeStrokeWidth" format="dimension" />
        <attr name="timeStrokeColor" format="color" />
        <attr name="stepShowOrder" format="boolean" />
        <attr name="stepPreColor" format="color" />
        <attr name="stepAfterColor" format="color" />
        <attr name="stepDividerAdapterName" format="string" />
        <attr name="stepIndexColor" format="color" />
    </declare-styleable>

    <declare-styleable name="TimeLineViewProxy">
        <attr name="visibleProxy" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
        <attr name="backgroundProxy" format="reference" />
    </declare-styleable>

</resources>